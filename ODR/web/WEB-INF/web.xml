<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>ord.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/ord/download</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login1</servlet-name>
        <servlet-class>ord.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login1</servlet-name>
        <url-pattern>/ord/login1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>ord.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/ord/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>server1</servlet-name>
        <servlet-class>ord.Server</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>server1</servlet-name>
        <url-pattern>/ord/server1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>ord.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/ord/upload</url-pattern>
    </servlet-mapping>
</web-app>
